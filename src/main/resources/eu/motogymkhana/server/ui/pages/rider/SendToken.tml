<html t:type="layout" t:title="Motogymkhana Sign In"
      t:pageTitle="Members login"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
 
 <h2>${title}</h2>
<p>${message}</p>
<BR></BR><BR></BR>
	<form t:type="form" class="full-form" t:id="registerForm">
		<t:errors />	
		<table>
			<tr>
				<th>
					<label t:type="label" t:for="username">email:</label>
				</th>
				<td>
					<input t:type="textfield" t:id="username" />
				</td>	
			</tr>
		<tr>
				<th><label t:type="label" t:for="token">token:</label></th>
				<td><input t:type="textfield" t:id="token"/></td>
			</tr>
				<tr>
				<th><label t:type="label" t:for="password">password:</label></th>
				<td><input type="password" t:type="passwordfield" t:id="password"/></td>
			</tr>
			<tr>
				<th><label t:type="label" t:for="password">password repeat:</label></th>
				<td><input type="password" t:type="passwordfield" t:id="passwordRepeat"/></td>
			</tr>
		</table>
		
		<div class="form-submit">
			<input type="submit" value="Register" />
		</div>
			 
	</form>
  
</html>
